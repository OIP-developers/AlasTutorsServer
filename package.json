{
  "name": "xyz",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "node -r dotenv/config dist/app.js",
    "build": "npm run clean && npm run build-ts",
    "build-ts": "tsc",
    "clean": "rimraf ./dist",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --exit-child --watch src ./src/app.ts",
    "start:prod": "cross-env NODE_ENV=production ts-node-dev --respawn --transpile-only --exit-child --watch src ./src/app.ts",
    "production": "node dist/app.js",
    "production:pm2": "pm2 start --name culturefy-backend dist/app.js",
    "seed:roles": "ts-node-dev src/seed/index.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start:pm2": "pm2 start --name ALAS_API npm -- run start:dev",
    "pm2:status": "pm2 status",
    "pm2:stop": "pm2 stop ALAS_API",
    "pm2:restart": "pm2 restart ALAS_API"
  },
  "author": "Faizan Ahmed",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/eslint": "^8.4.10",
