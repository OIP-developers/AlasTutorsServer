{
  "name": "zipAway-backend",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/app.js",
  "scripts": {
    "start": "npm run build && npm run generate && npm run serve",
    "serve": "node -r dotenv/config build/src/app.js",
    "build": "npm run clean && npm run build-ts",
    "build-ts": "tsc",
    "clean": "rimraf ./build/src && rimraf ./build/prisma",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --exit-child src/app.ts",
    "start:prod": "cross-env NODE_ENV=production ts-node-dev --respawn src/app.ts",
    "start:pm2": "pm2 start --name 'ZIPAWAY_API' build/src/app.js",
    "seed": "prisma db seed",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev --name init",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "Faizan Ahmed",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.321.1",
    "@aws-sdk/lib-storage": "^3.370.0",
    "@prisma/client": "^4.2.1",
    "@sendgrid/mail": "^7.7.0",
    "@types/express-serve-static-core": "^4.17.24",
    "@types/node-geocoder": "^4.2.1",
    "archiver": "^5.3.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "cloudinary": "^1.30.1",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "geolib": "^3.3.3",
    "google-auth-library": "^9.0.0",
    "helmet": "^4.6.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.8",
    "node-cron": "^3.0.1",
    "node-geocoder": "^4.2.0",
    "passport-facebook": "^3.0.0",
    "redis": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.6",
    "socket.io": "^4.5.3",
    "stripe": "^11.4.0",
    "uuid": "^9.0.0",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.1.7",
    "@types/archiver": "^5.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-formidable": "^1.2.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.10.1",
    "@types/node-cron": "^3.0.2",
    "@types/uuid": "^9.0.2",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.13",
    "prisma": "^4.8.0",
    "prisma-erd-generator": "^1.2.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  }
}
