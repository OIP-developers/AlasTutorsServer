    "@typescript-eslint/parser": "^5.47.1",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.30.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.30.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "helmet": "^4.6.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lint": "^0.8.19",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.8",
    "nodemailer": "^6.9.7",
    "stripe": "^10.6.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
